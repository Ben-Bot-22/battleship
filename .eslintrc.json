{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "overrides": [
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "prettier"
    ],
    "rules": {
        "max-len": 0,
        "no-unused-vars": 1,
        "no-trailing-spaces": 0,
        "comma-dangle": 0,
        "spaced-comment": 1,
        "object-shorthand": 1,
        "prefer-arrow-callback": 1,
        "import/prefer-default-export": 0,
        "prefer-const": 1,
        "no-undef": 1,
        "@typescript-eslint/no-non-null-assertion": 0
    }
}
